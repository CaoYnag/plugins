[Unit]
Description=CNI dhhp service socket
Documentation=https://github.com/containernetworking/plugins/tree/master/plugins/ipam/cdfh
PartOf=cni-dhhp.service

[Socket]
ListenStream=/run/cni/dhhp.sock
SocketMode=0660
SocketUser=root
SocketGroup=root
RemoveOnStop=true

[Install]
WantedBy=sockets.target
